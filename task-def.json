{
  "family": "todolist-app",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "todolist-app",
      "image": "637423569589.dkr.ecr.ap-southeast-2.amazonaws.com/todo-list-app:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/todolist-task-definition",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::637423569589:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::637423569589:role/ecsTaskExecutionRole",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-006ac3329aa1d5d9a",
        "subnet-0e04072d03b780b0a",
        "subnet-02d90eb1dad5ce089"
      ],
      "securityGroups": [
        "sg-0e92997243aad0339"
      ],
      "assignPublicIp": "ENABLED"
    }
  }
}
